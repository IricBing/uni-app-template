{
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "demo",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/user/register/index",
      "name": "app/register",
      "style": {
        "navigationBarTitleText": "注册",
        "navigationBarTextStyle": "black"
      },
      "meta": {
        "guard": [
          "userRegister"
        ]
      }
    },
    {
      "path": "pages/user/login/index",
      "name": "app/login",
      "style": {
        "navigationBarTitleText": "登录"
      }
    }
  ],
  "subpackages": [
    {
      "root": "subpackages/app1",
      "name": "app1",
      "pages": [
        {
          "path": "pages/user/register/index",
          "name": "subapp1/register",
          "style": {
            "navigationBarTitleText": "注册"
          }
        },
        {
          "path": "pages/user/login/index",
          "name": "subapp1/login",
          "style": {
            "navigationBarTitleText": "登录"
          }
        }
      ]
    },
    {
      "root": "subpackages/app2",
      "name": "app2",
      "pages": [
        {
          "path": "pages/user/register/index",
          "name": "subapp2/register",
          "style": {
            "navigationBarTitleText": "注册"
          }
        },
        {
          "path": "pages/user/login/index",
          "name": "subapp2/login",
          "style": {
            "navigationBarTitleText": "登录"
          }
        }
      ]
    }
  ]
}